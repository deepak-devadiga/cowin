{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "Analytics and Vaccine APIs",
      "urls": ["https://cdn-api.co-vin.in/api", "https://api.covid19india.org"],
      "cacheConfig": {
        "maxAge": "30m",
        "strategy": "performance",
        "maxSize": "10"
      }
    }
  ]
}
